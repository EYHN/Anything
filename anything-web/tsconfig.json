{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "esnext",
    "jsx": "preserve",
    "esModuleInterop": true,
    "strictNullChecks": true,
    "experimentalDecorators": true,
    "allowSyntheticDefaultImports": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "moduleResolution": "node",
    "typeRoots": [
        "./node_modules/@types"
    ]
  }
}